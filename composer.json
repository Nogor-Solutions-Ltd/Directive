{
    "name": "nogorsolutionsltd/directives",
    "description": "A custom directives package for laravel",
    "license": "MIT",
    "homepage": "https://www.nogorsolutions.com/",
    "support": {
        "issues": "https://github.com/Nogor-Solutions-Ltd/directives/issues",
        "source": "https://github.com/Nogor-Solutions-Ltd/directives"
    },
    "authors": [{
        "name": "Sarfaraz Muhammad Sajib",
        "email": "hey@m-sarfaraz.com"
    }],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "NogorSolutionsLTD\\Directives\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "NogorSolutionsLTD\\Directives\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}